@import 'style/colorui.wxss';
@import '/style/coloricon.wxss';
@import 'style/icon.wxss';
@import 'style/ql.wxss';
@import "/wxParse/wxParse.wxss";
/* reset */

page {
  background-color: #ededed;
  height: 100%;
  font-size: 30rpx;
  line-height: 1.6;
}

/* lib */

.strong {
  font-weight: bold;
}

.tc {
  text-align: center;
}

/* page */

.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}


button {
  background: white;
  border-radius: 0;
}

button::after {
  border: none;
}
