.progress-bar {
  height: 8rpx;
  border-radius: 4rpx;
  background: linear-gradient(#ffce76, #ffa000);
  width: 100%;
}

.b-page {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 32rpx;
  align-items: center;
  height: 100%;
}

.pre {
  color: #ffa000;
}

.pre::before {
  content: "\e660上一题";
  font-family: "iconfont" !important;
}

.next {
  color: #ffa000;
}

.next::after {
  content: "\e65f下一题";
  font-family: "iconfont" !important;
}

.submit {
  color: #ffa000;
}

.item-hover {
  color: #c5740b;
}
.bottom {
  width: 100%;
  background: white;
  display: flex;
  height: 100%;
}