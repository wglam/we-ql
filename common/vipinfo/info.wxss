.title {
  text-align: center;
  padding-top: 12rpx;
  padding-bottom: 12rpx;
}
.pro-panel {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.pro-item {
  width: 45%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 8rpx;
  border: 2rpx solid white;
  background-color: white;
}

.pro-item-img {
  width: 100%;
}

.pro-item-name {
  padding: 12rpx;
  text-align: center;
}